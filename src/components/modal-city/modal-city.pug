-
  const cities = [
    'Москва',
    'Санкт-Петербург',
    'Таганрог',
    'Краснодар',
    'Сочи',
    'Нижний Новгород',
  ]

mixin modal-city()
  +modal.modal-city
    .modal-city__inner
      .modal-city__title Укажите свой город
      .modal-city__grid
        each item in cities
          .modal-city__el
            span=item